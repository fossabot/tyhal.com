{
  "name": "tyhal",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.1.3",
    "bootswatch": "^4.1.3",
    "npm": "^6.3.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-history": "^0.18.2",
    "react-router": "3.2.1",
    "react-scripts": "1.1.4",
    "reactstrap": "^6.3.0"
  },
  "devDependencies": {
    "node-sass-chokidar": "^1.3.3",
    "node-sass-tilde-importer": "^1.0.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --importer=node_modules/node-sass-tilde-importer src/ -o src/gencss",
    "watch-css": "npm run build-css && node-sass-chokidar --importer=node_modules/node-sass-tilde-importer src/ -o src/gencss --watch --recursive",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
